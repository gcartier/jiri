(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: jiri description-file: {File :context ".package"}
    (<*>                              tag-reference: {File :context "jiri.project"})
    (<Project-Directory>              directory: {Directory :context "src"}
      (<Project-Directory>            directory: {Directory :context "src" "jiri"}
        (<Project-File>               source: {File :context "src" "jiri" "product.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "base.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "color.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "crash.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "draw.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "features.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "font.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "foreign.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "geometry.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "git-interface.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "git.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "install.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "launch.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "release"})
        (<Project-File>               source: {File :context "src" "jiri" "setup.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "structure.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "syntax.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "uninstall.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "view.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "window.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "windows.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "work.scm"})
        (<Project-File>               source: {File :context "src" "jiri" "devel.scm"}))))))
