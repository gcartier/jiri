;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: jiri       description-file: {File :context ".package"}
    (<*>                                    tag-reference: {File :context "jiri.project"})
    (<Project-Directory>                    directory: {Directory :context "src"}
      (<Project-Directory>                  directory: {Directory :context "src" "jiri"}
        (<Jazz-File>       name: structure  source: {File :context "src" "jiri" "structure.jazz"})
        (<Jazz-File>       name: work       source: {File :context "src" "jiri" "work.jazz"})
        (<Jazz-File>       name: view       source: {File :context "src" "jiri" "view.jazz"})
        (<Jazz-File>       name: settings   source: {File :context "src" "jiri" "settings.jazz"})
        (<Jazz-File>       name: base       source: {File :context "src" "jiri" "base.jazz"})
        (<Jazz-File>       name: update     source: {File :context "src" "jiri" "update.jazz"})
        (<Jazz-File>       name: syntax     source: {File :context "src" "jiri" "syntax.jazz"})
        (<Jazz-File>       name: connection source: {File :context "src" "jiri" "connection.jazz"})
        (<Jazz-File>       name: section    source: {File :context "src" "jiri" "section.jazz"})
        (<Jazz-File>       name: manager    source: {File :context "src" "jiri" "manager.jazz"})
        (<Jazz-File>       name: task       source: {File :context "src" "jiri" "task.jazz"})
        (<Jazz-File>       name: manage     source: {File :context "src" "jiri" "manage.jazz"})
        (<Jazz-File>       name: monitor    source: {File :context "src" "jiri" "monitor.jazz"})
        (<Jazz-File>       name: _jiri      source: {File :context "src" "jiri" "_jiri.jazz"})
        (<Jazz-File>       name: remote     source: {File :context "src" "jiri" "remote.jazz"}))))))
